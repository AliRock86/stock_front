AEA;AACA;AACA;AACA;;;;;AAKA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;;;;;;;;AASA;;;AAGA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;;AAGA;;;AAGA;;AAEA;;;AAGA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;;;;;AAKA;AACA;AACA;AACA;;;;;AAKA;;AAEA;AACA;;;;AAIA;AACA;AACA;AACA;;AAEA;AACA","file":"offers.vue","sourceRoot":"src/components","sourcesContent":["<template>\n\n\n  <div style=\"background-color:#eeee\" >\n\n    <div class=\"hidden-md-and-down search_component\" row wrap style=\"height:300px\">\n<!-- <v-parallax src=\"https://cdn.vuetifyjs.com/images/parallax/material.jpg\"  style=\"height:300px\"></v-parallax> -->\n    </div>\n\n\n    <div style=\"background-color:#fff\">\n      <v-container>\n        <br>\n\n        <v-layout row wrap>\n\n\n          \n\n\n          <v-flex xs12 md3 sm3 class=\"hj\">\n            <p class=\"lap_ser hidden-md-and-down\">البحث</p>\n            <autocomplete :search=\"filter\" placeholder=\"البحث\" @submit=\"search_select\"\n              @change=\"search_fun(search.sub_cat_id,provinces_id,search.text_titel)\"></autocomplete>\n          </v-flex>\n\n\n           <v-flex xs12 md3 sm3  class=\"hj\" pr-4 >\n            <p class=\"lap_ser hidden-md-and-down\">القسم</p>\n            <treeselect :multiple=\"true\" search-nested :options=\"options\" :disable-branch-nodes=\"true\"\n              v-model=\"search.sub_cat_id\" placeholder=\" اختار القسم\" />\n          </v-flex>\n\n\n          <v-flex xs12 md3 sm3   class=\"hj\" pr-4 >\n            <p class=\"lap_ser hidden-md-and-down\">المحافظه</p>\n            <treeselect  :options=\"options_provinces\" :flat=\"true\"\n              v-model=\"provinces_id\" :auto-load-root-options=\"false\" :multiple=\"true\" placeholder=\"اختر المحافظة\"\n              height=\"30px\" required />\n          </v-flex>\n\n\n         \n\n\n\n\n          <v-flex xs12 md3 sm3 pt-2 class=\"hj\">\n            <br>\n            <v-btn class=\"ma-2\" tile outlined color=\"#ff6600\" width=\"200\" height=\"50\" pt-4\n              @click=\"search_fun(search.sub_cat_id,provinces_id,search.text_titel)\">\n              <v-icon right>fas fa-search</v-icon> البحث\n            </v-btn>\n          </v-flex>\n\n        </v-layout>\n\n\n\n      </v-container>\n\n    </div>\n\n\n\n\n    <div>\n      <v-container fluid=\"\">\n        <v-row justify=\"center\">\n    <!-- partion one in the right for category -->\n          <v-flex xs12  xl2 md2 sm3 pa-3 v-show=\"!loading\" class=\"hidden-md-and-down\">\n            <filterCard :categores=\"categores\" \n    \n           :options_provinces=\"options_provinces\"\n           :provinces=\"provinces\"\n           \n             @inputData=\"setCategory\"></filterCard>\n\n          </v-flex>\n          <v-flex xs12 xl2 md2 sm3 pa-3 v-show=\"loading\">\n            <v-skeleton-loader ref=\"skeleton\" type=\"article\" class=\"mx-auto\"></v-skeleton-loader>\n            <v-skeleton-loader v-for=\"a in 6\" :key=\"a\" ref=\"skeleton\" type=\"list-item-two-line\" class=\"mx-auto\">\n            </v-skeleton-loader>\n\n          </v-flex>\n          <!-- partiton tow item details in center -->\n          <v-flex xs12 xl8 md9 sm9 pa-3 v-if=\"!loading\">\n\n\n\n            <cardItem :items=\"items\">\n            </cardItem>\n\n\n\n            <v-layout row wrap>\n              <v-row justify=\"center\" pa-5 style=\"margin-top:20px\">\n\n                <label class=\"mybooking-title\" v-show=\"error_msg\">\n                  <v-icon color=\"warning\"> fas fa-exclamation-triangle</v-icon>\n                  لا توجد حجوزات\n                </label>\n                <v-btn color=\"gray\" @click=\"getMore_page\" v-show=\"getmore\" text=\"\">\n                  <v-icon size=\"40\">fas fa-chevron-circle-down</v-icon>\n                </v-btn>\n\n              </v-row>\n            </v-layout>\n\n          </v-flex>\n          <v-flex v-else xs12 xl8 md9 sm9 pa-3>\n            <v-flex xs12 sm12 md12 style=\"width:100%;padding-bottom:2%\" v-for=\"a in 3\" :key=\"a\">\n              <v-card>\n                <v-skeleton-loader ref=\"skeleton\" type=\"article\" class=\"mx-auto\"></v-skeleton-loader>\n                <v-skeleton-loader ref=\"skeleton\" type=\"list-item-two-line\" class=\"mx-auto\"></v-skeleton-loader>\n              </v-card>\n            </v-flex>\n\n\n          </v-flex>\n\n\n\n        </v-row>\n\n\n\n        <v-layout>\n\n\n        </v-layout>\n      </v-container>\n    </div>\n  </div>\n\n\n</template>\n<script>\n  import Treeselect from '@riophae/vue-treeselect'\n  // import the styles\n  import '@riophae/vue-treeselect/dist/vue-treeselect.css'\n  \n\n  import cardItem from './sub_components/items_card'\n  import filterCard from './sub_components/filter'\n\n\n  const axios = require('axios');\n\n  export default {\n    data() {\n\n\n    \n\n      return {\n        search: [{\n          sub_cat_id: null,\n        }],\n        id: 0,\n        govs: [{\n          id: '1',\n          name: 'Bagdad',\n        }],\n        sub_cat: [{\n          id: '',\n          lable: ''\n        }],\n        urL: 'http://109.224.53.155',\n        options: [],\n        categores:[],\n        optionss: null,\n        options_provinces: [],\n        // provinces_id: '',\n        items: [],\n        getmore: false,\n        loading: false,\n        value: [],\n        provinces: [],\n        current_page: 0,\n        last_page: 0,\n        //options: [],\n        xx: null,\n        items_serch_titel: [],\n        error_msg: false,\n        selected_sub_cat: [],\n        numof_subcat_selected: -1,\n        rating: 5,\n        provinces_id:null\n      }\n    },\n    mounted() {\n\n\n      this.getitems();\n      this.getProvinces();\n      this.getCategory();\n      this.Get_All_items_title();\n\n    },\n    methods: {\n setCategory(sub_Cat_ides,rovinces_id) {\n   \n   rovinces_id,\n   this.selected_sub_cat = sub_Cat_ides;\n    this.search.sub_cat_id =  sub_Cat_ides;\n\n  var len;\nif(this.selected_sub_cat==null){\nlen=0;\n}else{len=this.selected_sub_cat.length ;}\n    \n        if (len != this.numof_subcat_selected && len!= 0) {\n\n          this.numof_subcat_selected = this.selected_sub_cat.length;\n          this.search.sub_cat_id = this.selected_sub_cat;\n          this.search.text_titel = null;\n          \n\n       \n       this.search_fun(this.selected_sub_cat, this.provinces_id, null)\n        } else if (len == 0 && this.numof_subcat_selected  != -1) {\n\n          this.numof_subcat_selected = -1;\nthis.current_page=this.current_page-1;\n          this.getitems();\n\n\n        }\n    },\n      getCategory() {\n\n        var url = this.urL + \"/getInfo/Category\";\n        axios.get(url).then(response => {\n          this.categores = response.data.Data;\n          var i = 0;\n          for (i = 0; i <= this.categores.length; i++) {\n          \n this.categores[i].id=i+100;\n  \n\n            this.sub_cat = this.categores[i].sub_category.map(item => {\n              return {\n                id: item.id,\n                label: item.name\n              };\n            });\n            var j = 0;\n            for (j = 0; j <= this.categores[i].sub_category.length; j++) {\n              //  this.sub_category[j].id;\n              // this.sub_cat[j].id=this.categores[j].id+'_'+this.sub_cat[j].id;\n\n              //                 if(this.sub_cat[j].id)\n              //                 {\n              // alert(this.sub_cat[j].label);\n              //                 }\n\n            }\n\n\n\n            //Push Categories in TreeSelect\n            this.options.push({\n                id: 'x' + this.categores[i].id,\n                label: this.categores[i].name,\n                children: this.sub_cat\n\n                //    this.sub_cat\n\n\n              }\n\n            );\n\n\n\n          }\n        \n\n         \n\n\n        });\n\n\n      },\n      getitems() {\n\n        \n        this.loading = true;\n        this.error_msg = false;\n        this.getmore = false;\n         this.$http.get(\"/v2/items/search?filter[Getoffers]=1&sort=-id&page=\" +this.current_page+1).then(response => {\n          this.items = response.data.data;\n\n          this.current_page = response.data.Data.current_page;\n          this.last_page = response.data.Data.last_page;\n          //hidden getmore button if there is no more \n          if (this.current_page == this.last_page) {\n            this.getmore = false;\n          } else {\n            this.getmore = true\n          }\n          if (this.items.length < 1) {\n            this.error_msg = true\n          }\n        }).catch(error => {\n         error\n        }).finally(d => {\n          d,\n          this.loading = false;\n        });\n      },\n\n\n\n      getitemsByCat_id() {\n        var url = this.urL + \"/getByCatId/\" + this.id;\n        axios.get(url).then(response => {\n          this.items = response.data.Data[0].sub_category;\n          //console.log(this.items)\n\n\n        }).catch(error => {\n       error\n        });\n      },\n\n      Get_All_items_title() {\n\n        var url = this.urL + \"/getItemOffers?page=1\";\n        axios.get(url).then(response => {\n          var items_serch = response.data.Data.data;\n\n          var i = 0;\n\n          for (i = 0; i < items_serch.length; i++) {\n\n            this.items_serch_titel.push(items_serch[i].name);\n\n          }\n\n\n        }).catch(error => {\nerror\n        });\n\n\n      },\n      filter(input) {\n\n        if (input.length < 1) {\n          this.search.text_titel = null;\n          return []\n        }\n        return this.items_serch_titel.filter(item => {\n\n          this.search.text_titel = input;\n          return item.toLowerCase()\n            .startsWith(input.toLowerCase())\n        })\n      },\n      search_select(input_search) {\n\n\n\n        this.search_fun(this.search.sub_cat_id, this.provinces_id, input_search);\n        // this.$router.push({name: 'search', params: {cat_id:this.search.sub_cat_id+'',\n        //     gov_id:'1',\n        //     tit: input_search}})\n\n      },\n\n\n\n\n\n\n\n\n\n      search_fun(sub_Cat_id, provinces_id, title) {\n        this.xx = title;\n        this.getmore = false;\n\n\nthis.loading=true;\n        if (title==undefined) {\n          title = null;\n\n        } else {\n        // title= title;\n        }\n\n\n\n\n       if (sub_Cat_id == undefined) {\n          sub_Cat_id = null;\n        } else if (sub_Cat_id.length == 0) {\n          sub_Cat_id = null\n        } else {\n        //  sub_Cat_id = sub_Cat_id;\n        }\n\n        if(this.search.sub_cat_id==undefined)\n        {\n          this.sub_Cat_id = null;\n        }\n\n\n\n\n        if (provinces_id == undefined) {\n          provinces_id = null;\n        }\n        if (isNaN(this.xx)) {\n          this.xx = null;\n\n        }\n\n        // alert(this.search.sub_cat_id.length);\n\n        axios({\n            method: 'get',\n            url: this.urL + \"/search/\" + sub_Cat_id + \"/\" + provinces_id + \"/\" + title + \"\"\n\n          })\n          .then(response => {\n             this.error_msg=false;\n            this.items = response.data.Data.data;\n            this.xx = null;\n            if (this.items.length < 1) {\n              this.error_msg = true\n            }\n\n          })\n          .catch(error => {\n            error,\n            this.error_msg=true\n          }).finally(d=>{\n            d,\n            this.loading=false\n          });\n\n      },\n\n\n\n\n\n\n\n\n      getProvinces() {\n\n\n        var url = this.urL + \"/getInfo/Province/1\";\n        axios.get(url).then(response => {\n          this.provinces = response.data.Data;\n          var i = 0;\n          for (i = 0; i <= this.provinces.length; i++) {\n\n            this.options_provinces.push({\n              id: this.provinces[i].id,\n              label: this.provinces[i].province_name,\n            });\n\n\n          }\n\n\n        });\n\n      },\n     \n\n      getdays(date) {\n\n        var days = [\"السبت\", \"الاحد\", \"الاثنين\", \"الثلاثاء\", \"الاربعاء\", \"الخميس\", \"الجمعة\"];\n        if (date.every_day == 0) {\n          if (days[date.from_day] == days[date.to_day]) {\n            return days[date.from_day]\n          } else {\n            return days[date.from_day] + \" - \" + days[date.to_day];\n          }\n\n\n        } else {\n          return \"كل الايام\";\n        }\n      },\n      getMore_page() {\n        this.loading = true;\n        if (this.current_page == this.last_page) {\n          this.getmore = false;\n          this.loading = false;\n        } else {\n\n\n          var url = this.urL + \"/getItemOffers?page=\" + (this.current_page + 1);\n          axios.get(url)\n          .then(response => {\n            this.items.push.apply(this.items, response.data.Data.data);\n            this.current_page = response.data.Data.current_page;\n            this.last_page = response.data.Data.last_page;\n            this.loading = false;\n\n\n            if (this.current_page == this.last_pagse) {\n              this.getmore = false;\n            } else {\n              this.getmore = true\n            }\n\n\n\n\n          }).catch(error => {\n           error,\n            this.loading = false;\n          });\n\n\n\n\n        }\n\n      }\n    },\n    \n  \n   \n    components: {\n      Treeselect,\n      cardItem,\n      filterCard\n\n    }\n  };\n</script>"]}]}