AACA;AACA;AACA;AACA;;AAEA;;AAEA;;;AAGA;AACA;;;;AAIA;AACA;;;;;AAKA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;;;;;;AAOA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;;AAGA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;;AAGA;;;AAGA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;;;;;AAKA;;AAEA;;;;AAIA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;;AAGA;AACA;AACA;;;;;AAKA;;AAEA;;AAEA;AACA;AACA;;;;AAIA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;;;;;AAKA;;AAEA;AACA;;AAEA;;;;AAIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA","file":"search.vue","sourceRoot":"src/components","sourcesContent":["<template>\n\n\n  <div style=\"background-color:#eeee\">\n\n    <div class=\"hidden-md-and-down search_component\" row wrap style=\"height:300px\">\n\n\n    </div>\n\n\n    <div style=\"background-color:#fff\">\n      <v-container>\n        <br>\n\n        <v-layout row wrap>\n\n\n          <v-flex xs12 md3 sm3 class=\"hj\">\n            <p class=\"hidden-md-and-down lap_ser\">البحث</p>\n            <!-- <v-text-field label=\"البحث\" solo v-model=\"search.text_titel\"></v-text-field> -->\n            <autocomplete :search=\"filter\" placeholder=\"البحث\" @submit=\"search_select\"\n              @change=\"search_fun(search.sub_cat_id,provinces_id,search.text_titel)\"></autocomplete>\n          </v-flex>\n\n\n          <v-flex xs12 md3 sm3 pr-4 class=\"hj\">\n            <p class=\"hidden-md-and-down lap_ser\">القسم</p>\n            <treeselect :multiple=\"true\" search-nested :options=\"options\" :disable-branch-nodes=\"true\"\n              v-model=\"search.sub_cat_id\" placeholder=\" اختار القسم\" />\n          </v-flex>\n\n\n          <v-flex xs12 md3 sm3 pr-4 class=\"hj\">\n            <p class=\"hidden-md-and-down lap_ser\">المحافظه</p>\n            <treeselect :options=\"options_provinces\" :flat=\"true\" v-model=\"provinces_id\" :auto-load-root-options=\"false\"\n              :multiple=\"true\" placeholder=\"اختر المحافظة\" height=\"30px\" required />\n          </v-flex>\n\n\n          \n\n\n\n\n          <v-flex xs12 md3 sm3 pt-2 class=\"hj\">\n            <br>\n            <v-btn class=\"ma-2\" tile outlined color=\"#ff6600\" width=\"200\" height=\"50\" pt-4\n              @click=\"search_fun(search.sub_cat_id,provinces_id,search.text_titel)\">\n              <v-icon right>fas fa-search</v-icon> البحث\n            </v-btn>\n          </v-flex>\n\n        </v-layout>\n\n\n\n      </v-container>\n\n    </div>\n\n\n\n\n    <div>\n      <v-container fluid=\"\">\n        <v-row justify=\"center\">\n          <!-- partion one in the right for category -->\n          <v-flex xs12 xl2 md2 sm3 pa-3 v-show=\"!loading\" class=\"hidden-md-and-down\">\n\n            <filterCard :categores=\"categores\" :options_provinces=\"options_provinces\" :provinces=\"provinces\"\n              @inputData=\"setCategory\" :orderby=\"orderby\" :selected_sub_cat=\"selected_sub_cat\"></filterCard>\n          </v-flex>\n          <v-flex xs12 xl2 md2 sm3 pa-3 v-show=\"loading\">\n            <v-skeleton-loader ref=\"skeleton\" type=\"article\" class=\"mx-auto\"></v-skeleton-loader>\n            <v-skeleton-loader v-for=\"a in 6\" ref=\"skeleton\" type=\"list-item-two-line\" class=\"mx-auto\" :key=\"a\">\n            </v-skeleton-loader>\n\n          </v-flex>\n          <!-- partiton tow item details in center -->\n          <v-flex xs12 xl8 md9 sm9 pa-3 v-if=\"!loading\">\n\n            <cardItem :items=\"items\">\n            </cardItem>\n\n\n            <v-layout row wrap>\n              <v-row justify=\"center\" pa-5 style=\"margin-top:20px\">\n\n                <label class=\"mybooking-title\" v-show=\"error_msg\">\n                  <v-icon color=\"warning\"> fas fa-exclamation-triangle</v-icon>\n                  لا توجد حجوزات\n                </label>\n                <v-btn color=\"gray\" @click=\"getMore_page\" v-show=\"getmore\">تحميل المزيد </v-btn>\n\n              </v-row>\n            </v-layout>\n\n          </v-flex>\n          <v-flex v-else xs12 xl8 md9 sm9 pa-3>\n            <v-flex xs12 sm12 md12 style=\"width:100%;padding-bottom:2%\" v-for=\"a in 3\" :key=\"a\">\n              <v-card>\n                <v-skeleton-loader ref=\"skeleton\" type=\"article\" class=\"mx-auto\"></v-skeleton-loader>\n                <v-skeleton-loader ref=\"skeleton\" type=\"list-item-two-line\" class=\"mx-auto\"></v-skeleton-loader>\n              </v-card>\n            </v-flex>\n\n\n          </v-flex>\n\n\n\n        </v-row>\n\n\n\n        <v-layout>\n\n\n        </v-layout>\n      </v-container>\n    </div>\n  </div>\n\n\n</template>\n<script>\n  import Treeselect from '@riophae/vue-treeselect'\n  // import the styles\n  import '@riophae/vue-treeselect/dist/vue-treeselect.css'\n  import cardItem from './sub_components/items_card'\n  import filterCard from './sub_components/filter'\n  import func from \"../fun\"\n  import {\n    EventBus\n  } from '../event-bus.js';\n\n  const axios = require('axios');\n\n  export default {\n    data() {\n\n\n\n      return {\n        search: [{\n          sub_cat_id: null,\n\n        }],\n        id: 0,\n        sub_cat: [{\n          id: '',\n          lable: ''\n        }],\n        urL: 'http://109.224.53.155',\n        items: [],\n        getmore: false,\n        loading: false,\n        value: [],\n        current_page: 1,\n        last_page: 0,\n        options: [],\n        xx: null,\n        provinces: [],\n        categores: [],\n        provinces_id: null,\n        items_serch_titel: [],\n        error_msg: false,\n        selected_sub_cat: [],\n        options: [],\n        optionss: null,\n        options_provinces: [],\n        orderby: '',\n        numof_subcat_selected: -1,\n        urlNow: '',\n        orderbyselected: '',\n        long: 0,\n        lat: 0\n      }\n    },\n    mounted() {\n\n      this.getCategory();\n      this.Get_All_items_title();\n      this.getProvinces();\n\n    },\n\n\n    methods: {\n      setlocation(lat, lng) {\n        this.getNearestItems(lat, lng);\n      },\n\n\n      getNearestItems(lat, lng) {\n        var url = this.urL + \"/getNearestItems/\" + lng + \"/\" + lat;\n        this.urlNow = url;\n        this.loading = true;\n        this.error_msg = false;\n        this.getmore = false;\n        axios.get(url).then(response => {\n          this.items = response.data.Data.data;\n\n          this.current_page = response.data.Data.current_page;\n          this.last_page = response.data.Data.last_page;\n          //hidden getmore button if there is no more \n          if (this.current_page == this.last_page) {\n            this.getmore = false;\n          } else {\n            this.getmore = true\n          }\n\n          if (this.items.length < 1) {\n            this.error_msg = true;\n          }\n\n        }).catch(error => {\n\n        }).finally(d => {\n          this.loading = false\n        });\n      },\n\n      setCategory(sub_Cat_ides, orderby, lat, long) {\n        this.long = long;\n        this.lat = lat;\n        // if (provinces_id.length > 0) {\n        //   this.search.provinces_id = provinces_id;\n        // } else {\n        //   this.search.provinces_id = null;\n        // }\n\n\n        this.selected_sub_cat = sub_Cat_ides;\n        this.search.sub_cat_id = sub_Cat_ides;\n\n\n        if (this.orderby != orderby) {\n\n          this.orderby = orderby;\n          this.search_fun(this.selected_sub_cat, this.provinces_id, null)\n        }\n\n        if (this.selected_sub_cat.length != this.numof_subcat_selected && this.selected_sub_cat.length != 0) {\n\n          this.numof_subcat_selected = this.selected_sub_cat.length;\n          this.search.sub_cat_id = this.selected_sub_cat;\n          this.search.text_titel = null;\n\n\n\n          this.search_fun(this.selected_sub_cat, this.provinces_id, null)\n        } else if (this.selected_sub_cat.length == 0 && this.numof_subcat_selected != -1) {\n\n          this.numof_subcat_selected = -1;\n\n          this.search_fun_start();\n\n\n        }\n      },\n\n\n      getCategory() {\n\n        var url = this.urL + \"/getInfo/Category\";\n        axios.get(url).then(response => {\n          this.categores = response.data.Data;\n          var i = 0;\n          for (i = 0; i <= this.categores.length; i++) {\n            this.categores[i].id = i + 100;\n\n\n\n\n\n            this.sub_cat = this.categores[i].sub_category.map(item => {\n              return {\n                id: item.id,\n                label: item.name\n              };\n            });\n\n\n            this.options.push({\n                id: 'x' + this.categores[i].id,\n                label: this.categores[i].name,\n                children: this.sub_cat\n\n\n\n\n              }\n\n            );\n\n\n\n          }\n\n\n        }).finally(s => {\n\n\n\n        });\n\n\n      },\n\n\n\n      Get_All_items_title() {\n\n        var url = this.urL + \"/getLast?page=1\";\n        axios.get(url).then(response => {\n          var items_serch = response.data.Data.data;\n\n          var i = 0;\n\n          for (i = 0; i < items_serch.length; i++) {\n\n            this.items_serch_titel.push(items_serch[i].name);\n\n          }\n\n\n        }).catch(error => {\n\n        });\n\n\n      },\n\n\n\n      filter(input) {\n\n        if (input.length < 1) {\n          this.search.text_titel = null;\n          return []\n        }\n        return this.items_serch_titel.filter(item => {\n\n          //save the return text search\n          this.search.text_titel = input;\n\n\n          return item.toLowerCase()\n            .startsWith(input.toLowerCase())\n        })\n      },\n\n\n      search_select() {\n\n        //Seach  ThE  Selected Autocomplent titel\n        //  this.search_fun(this.search.sub_cat_id, null, input_search);\n\n      },\n\n\n      search_fun(sub_Cat_id, provinces_id, title) {\n\n        this.xx = title;\n        this.getmore = false;\n        this.error_msg = false\n        this.loading = true\n\n\n  if (title==undefined) {\n          title = null;\n\n        } else {\n         title= title;\n        }\n\n\n\n\n\n        if (sub_Cat_id == undefined) {\n          sub_Cat_id = null;\n        } else if (sub_Cat_id.length == 0) {\n          sub_Cat_id = null\n        } else {\n          sub_Cat_id = sub_Cat_id;\n        }\n\n        if(this.search.sub_cat_id==undefined)\n        {\n          this.sub_Cat_id = null;\n        }\n\n        \n\n\n\n\n\n        if (provinces_id == undefined) {\n          provinces_id = null;\n        }\n        if (isNaN(this.xx)) {\n          this.xx = null;\n\n        } else {\n\n        }\n\n\n        var header;\n        let self = this;\n\n\n\n        func.chekpermition().then(function (result) {\n          // Will return ['granted', 'prompt', 'denied']\n\n\n\n\n          if (result.state == \"granted\" && self.orderby == \"nearest\") {\n\n            header = {\n              long: self.long,\n              lat: self.lat\n            };\n          } else if (result.state == \"denied\" && self.orderby == \"nearest\") {\n            header = {};\n\n            self.error_msg = true\n            self.loading = false\n            return;\n            self.$swal(\"\", \"   يجب تفعيل الوصول للموقع\", 'warning')\n          } else if (result.state == \"prompt\" && self.orderby == \"nearest\") {\n\n            self.error_msg = true\n            self.loading = false\n            return;\n            header = {};\n            self.$swal(\"\", \"   فعل الوصول للموقع رجاءاً   \", 'warning')\n          } else {\n            header = {};\n\n          }\n\n\n\n\n\n\n          axios({\n              method: 'get',\n              url: self.urL + \"/search/\" + sub_Cat_id + \"/\" + provinces_id + \"/\" + title + \"/\" + self.orderby +\n                \"?page=\" + self.current_page,\n\n              headers: header\n            })\n            .then(response => {\n\n              self.error_msg = false\n              self.items = response.data.Data.data;\n\n              self.xx = null;\n              self.current_page = response.data.Data.current_page;\n              self.last_page = response.data.Data.last_page;\n\n              //hidden getmore button if there is no more \n              if (self.current_page == self.last_page) {\n                self.getmore = false;\n              } else {\n                self.getmore = true\n              }\n              if (self.items.length < 1) {\n                self.error_msg = true\n              }\n\n\n            })\n            .catch(\n              self.error_msg = true\n\n            ).finally(\n              self.loading = false\n            );\n        });\n\n      },\n      getProvinces() {\n\n\n        var url = this.urL + \"/getInfo/Province/1\";\n        axios.get(url).then(response => {\n          this.provinces = response.data.Data;\n          var i = 0;\n          for (i = 0; i <= this.provinces.length; i++) {\n\n            this.options_provinces.push({\n              id: this.provinces[i].id,\n              label: this.provinces[i].province_name,\n            });\n\n\n          }\n\n\n        });\n\n      },\n\n      getdays(date) {\n\n        var days = [\"السبت\", \"الاحد\", \"الاثنين\", \"الثلاثاء\", \"الاربعاء\", \"الخميس\", \"الجمعة\"];\n        if (date.every_day == 0) {\n          if (days[date.from_day] == days[date.to_day]) {\n            return days[date.from_day]\n          } else {\n            return days[date.from_day] + \" - \" + days[date.to_day];\n          }\n\n\n        } else {\n          return \"كل الايام\";\n        }\n      },\n\n\n      getMore_page() {\n        this.loading = true;\n        if (this.current_page == this.last_page) {\n          this.getmore = false;\n          this.loading = false;\n        } else {\n\n\n          this.current_page = this.current_page + 1;\n          //   this.search_fun(this.search.sub_cat_id, this.search.provinces_id, this.search.text_titel)\n\n\n\n\n        }\n\n      },\n\n\n\n    },\n    created() {\n\n      this.orderby = this.$route.params.orderby;\n      this.orderbyselected = this.orderby;\n      if (this.$route.params.cat_id != \"null\" && this.$route.params.cat_id != \"undefined\") {\n        var sub_ides = this.$route.params.cat_id.split(\",\");\n\n        for (var i = 0; i < sub_ides.length; i++) {\n          var sub_Cat_id = parseInt(sub_ides[i], 10);\n\n          this.selected_sub_cat.push(sub_Cat_id);\n        }\n\n\n        var provns = this.$route.params.gov_id.split(\",\");\n        var provinces_ides = [];\n        for (var ii = 0; ii < provns.length; ii++) {\n\n\n\n\n          provinces_ides.push(provns[ii]);\n\n        }\n\n        if (provinces_ides[0] != \"null\") {\n          this.provinces_id = provinces_ides;\n        }\n\n\n\n        //save the lenght of  sup cat selected\n        this.numof_subcat_selected = this.selected_sub_cat.length;\n\n        this.search.sub_cat_id = this.selected_sub_cat;\n\n      }\n      this.search_fun(this.$route.params.cat_id, this.$route.params.gov_id, this.$route.params.tit);\n\n      EventBus.$on('searchx', items => {\n        this.items = items;\n\n\n\n\n      });\n\n    },\n    computed: {\n\n      selected() {\n\n\n\n      },\n    },\n    watch: {\n      selected: 'search by sub_cat_id',\n    },\n    components: {\n      Treeselect,\n      cardItem,\n      filterCard\n\n\n    }\n  };\n</script>"]}]}