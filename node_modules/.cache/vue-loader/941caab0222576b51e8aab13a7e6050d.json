layout column wrap justify-center class=\"news\">\n    <v-flex xs12>\n      <v-row>\n        <label class=\"mybooking-title\">اخر الاخبار حول اضافة الحجوزات الاخيرة</label>\n\n      </v-row>\n    </v-flex>\n\n    <v-row>\n      <v-divider></v-divider>\n\n    </v-row>\n\n    <v-flex xs12>\n      <v-row>\n\n        <v-container>\n\n\n\n\n          <v-sheet class=\"mx-auto\">\n            <v-slide-group elevation=\"8\" show-arrows dir=\"ltr\">\n\n              <v-slide-item v-for=\"item in 20\" :key=\"item\" v-slot:default=\"{ active, toggle }\">\n                <v-card :class=\"news_card\">\n                  <v-card-title primary-title>\n                    مايو\n                  </v-card-title>\n                  <v-card-title primary-title>\n                    2019\n\n                  </v-card-title>\n                </v-card>\n              </v-slide-item>\n            </v-slide-group>\n          </v-sheet>\n\n\n\n\n\n\n        </v-container>\n      </v-row>\n    </v-flex>\n\n\n    <v-row class=\"mb-6\" justify=\"center\">\n\n\n    </v-row>\n\n    <v-row>\n    </v-row>\n    <v-flex xs12>\n\n\n      <v-row>\n        <v-container>\n          <v-layout row wrap>\n\n\n            <v-flex xs12 md4 v-for=\"item in items\" pa-3 v-if=\"!loading\">\n\n              <v-card class=\"up_event_card\" v-for=\"img  in item.images.slice(0,1)\"   v-bind:style=\"{ 'background-image':'url(http://109.224.53.155/'+img.image_path+'' }\">\n                <v-layout column wrap style=\"height:100%\">\n                  <v-row>\n                    <div class=\"up_event_card_date\">\n                      <span class=\"day\">{{getDay(item.created_at)}}</span><br>\n                      <span class=\"month\">{{getMonth(item.created_at)}}</span><br>\n                      <span class=\"year\">{{getYear(item.created_at)}}</span>\n                    </div>\n                  </v-row>\n                  <v-spacer></v-spacer>\n                  <v-container fluid class=\"up_event_card_bottom_layout\">\n                    <v-layout row wrap>\n                      <div style=\"text-align: start;\">\n                        <span class=\"up_event_card_text1\"> {{item.name}} </span><br>\n                        <span class=\"up_event_card_text2\"> {{item.offer_disc}} </span>\n                      </div>\n                      <v-spacer></v-spacer>\n                      <v-btn class=\"booking-btn\" :to=\"'/item/'+item.item_id\">\n                        <v-icon right=\"\">fas fa-bookmark</v-icon>\n                        طلب الحجز\n                      </v-btn>\n                    </v-layout>\n                  </v-container>\n\n                </v-layout>\n\n              </v-card>\n            </v-flex>\n\n\n            <v-flex xs12 md4 pa-3 v-for=\"a in 6\" v-if=\"loading\">\n\n              <v-card>\n                <v-skeleton-loader ref=\"skeleton\" type=\"article\" class=\"mx-auto\"></v-skeleton-loader>\n                <v-skeleton-loader ref=\"skeleton\" type=\"list-item-two-line\" class=\"mx-auto\"></v-skeleton-loader>\n              </v-card>\n\n\n\n            </v-flex>\n\n\n          </v-layout>\n        </v-container>\n      </v-row>\n    </v-flex>\n    <v-row justify=\"center\" class=\"mb-6\">\n\n      <v-row justify=\"center\" pa-5 style=\"margin-top:20px\">\n\n        <label class=\"mybooking-title\" v-show=\"error_msg\">\n          <v-icon color=\"warning\"> fas fa-exclamation-triangle</v-icon>\n          لا توجد حجوزات\n        </label>\n        <v-btn color=\"gray\" @click=\"getMore_page\" v-show=\"getmore\">تحميل المزيد </v-btn>\n\n      </v-row>\n\n    </v-row>\n  </v-layout>\n</template>\n\n<script>\n  export default {\n    data: () => ({\n      model: null,\n      news_card: 'news_card_inactive',\n      urL: 'http://109.224.53.155',\n      items: [],\n      current_page: 0,\n      last_page: 0,\n      getmore: true,\n      error_msg: false,\n      loading: false,\n    }),\n    mounted() {\n      this.getbooking();\n    },\n    methods: {\n      getbooking() {\n        this.loading = true;\n        this.error_msg = false;\n        this.getmore = false;\n        var url = this.urL + \"/getAllOffers?page=\" + (this.current_page + 1);\n        axios.get(url).then(response => {\n          this.items = response.data.Data.data;\n\n          this.current_page = response.data.Data.current_page;\n          this.last_page = response.data.Data.last_page;\n          if (this.current_page == this.last_page) {\n            this.getmore = false;\n          }\n\n        }).catch(error => {\n          alert(\"error\")\n        }).finally(d => {\n\n          this.loading = false;\n        });\n      },\n\n\n\n\n\n\n      getMore_page() {\n        this.loading = true;\n        if (this.current_page == this.last_page) {\n\n          this.loading = false;\n          this.getmore = false;\n        } else {\n\n\n          var url = this.urL + \"/getAllOffers?page=\" + (this.current_page + 1);\n          axios.get(url).then(response => {\n\n            this.items.push.apply(this.items, response.data.Data.data);\n            this.current_page = response.data.Data.current_page;\n            this.last_page = response.data.Data.last_page;\n            this.loading = false;\n\n          }).catch(error => {\n\n            this.error_msg = true;\n\n          });\n\n\n\n\n        }\n\n      },\n\n      getYear(date) {\n        var year = date.split(\"-\")[0]\n\n        return year;\n      },\n      getMonth(date) {\n        var month = date.split(\"-\")[1]\n        var months = [\"يناير\", \"فبراير\", \"مارس\", \"إبريل\", \"مايو\", \"يونيو\",\n          \"يوليو\", \"أغسطس\", \"سبتمبر\", \"أكتوبر\", \"نوفمبر\", \"ديسمبر\"\n        ];\n        return months[month - 1];\n      },\n      getDay(date) {\n\n        var day = date.split(\"-\")[2].split(\" \")[0]\n\n        return day;\n      }\n    },\n    components: {\n\n    }\n  }\n</script>"]}]}